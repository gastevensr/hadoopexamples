	static class MaxTemperatureMapperWIthCounters extends Mapper<LongWritable, Text, Text, IntWritable> {

	}

	@Override
	public int run(String[] arg0) throws Exception {
		return 0;
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
	}

}
